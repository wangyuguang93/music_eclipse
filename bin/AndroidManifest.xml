            android:priority="1000"
            android:name="com.checker.quanxian.WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="MyService"></service>
        <activity android:name="SousuoListFragment"></activity>
        <activity android:name="Login"></activity>
        <activity android:name="User_info"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name="com.checker.quanxian.PermissionActivity"></activity>
    </application>

</manifest>
